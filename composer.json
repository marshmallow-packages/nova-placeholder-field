{
    "name": "marshmallow/nova-placeholder",
    "description": "A placeholder field to just so some content on forms without any logic.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\Placeholder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\Placeholder\\FieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
